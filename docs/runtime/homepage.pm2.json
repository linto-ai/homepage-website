{
    "apps": [
        {
            "name": "homepage-runtime",
            "script": "node",
            "args": "app.js",
            "log_date_format": "YYYY-MM-DD HH:mm Z",
            "cwd": "PATH_TO_SCRIPT",
            "env": {
                "PUBLIC_DOWNLOAD_FOLDER": "SHARABLE_FOLDER",
                "ALLOWED_ORIGINS": "https://doc.linto.ai,https://www.linto.ai,https://linto.ai,https://gamma.linto.ai",
                "WEBSERVER_HTTP_PORT": 9009,
                "NODE_ENV": "production",
                "SMTP_HOST": "SMTP_HOST",
                "SMTP_PORT": 666,
                "SMTP_SECURE": true,
                "SMTP_REQUIRE_TLS": "true",
                "SMTP_AUTH": "noreply@you.com",
                "SMTP_PSWD": "crazypassword",
                "CONTACT_EMAIL": "contact@you.com",
                "NO_REPLY_EMAIL": "noreply@you.com"
            }
        }
    ]
}